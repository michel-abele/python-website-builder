{
    "init": {
        "python_required_modules": [
            "tqdm",
            "libsass",
            "jsmin"
        ],
        "directories_to_delete": [
            "./.git",
            "./temp",
            "./.vscode"
        ],
        "directories_to_create": [
            "./page",
            "./source",
            "./source/fonts",
            "./source/html",
            "./source/html/content",
            "./source/html/parts",
            "./source/img",
            "./source/js",
            "./source/js/backend",
            "./source/js/backend/content",
            "./source/js/backend/parts",
            "./source/js/frontend",
            "./source/js/frontend/content",
            "./source/js/frontend/parts",
            "./source/misc",
            "./source/sitemap",
            "./source/scss",
            "./source/scss/content",
            "./source/scss/parts"
        ],
        "directories_to_copy_source": [
            "./system/init/scss/framework/framework",
            "./system/init/js/frontend/framework/framework"
        ],
        "directories_to_copy_target": [
            "./source/scss/parts/framework",
            "./source/js/frontend/parts/framework"
        ],
        "files_to_delete": [
            "./.gitignore",
            "./LICENSE",
            "./README.md"
        ],
        "files_to_create": [
            "./source/html/content/index.html",
            "./source/js/backend/content/index.js",
            "./source/js/frontend/content/custom.js",
            "./source/misc/.htaccess",
            "./source/misc/robots.txt",
            "./source/scss/parts/main.scss"
        ],
        "files_to_copy_source": [
            "./system/init/scss/framework/framework.scss",
            "./system/init/js/frontend/framework/framework.js"
        ],
        "files_to_copy_target": [
            "./source/scss/content/framework.scss",
            "./source/js/frontend/content/framework.js"
        ]
    },
    "build": {
        "target_directory": "./page",
        "temp_directory": "./temp",
        "create_library_directories": [
            "./page/library",
            "./page/library/css",
            "./page/library/fonts",
            "./page/library/img",
            "./page/library/js",
            "./page/library/sitemaps"
        ],
        "library": {
            "directory_main": "./page/library",
            "directory_css": "./page/library/css",
            "directory_fonts": "./page/library/fonts",
            "directory_img": "./page/library/img",
            "directory_js": "./page/library/js",
            "directory_sitemaps": "./page/library/sitemaps"
        },
        "source": {
            "directory_html": "./source/html/content",
            "directory_js": "./source/js/frontend/content",
            "directory_scss": "./source/scss/content"
        },
        "partials": {
            "directory_html": "./source/html/parts",
            "directory_js": "./source/js/frontend/parts",
            "directory_scss": "./source/scss/parts"
        },
        "iso": {
            "639_2": "./system/config/iso-639_2.json",
            "639_3": "./system/config/iso-639_3.json",
            "3166-1_2": "./system/config/iso-3166-1_2.json",
            "3166-1_3": "./system/config/iso-3166-1_3.json"
        },
        "temp_directories": {
            "sitemaps": "./temp/sitemaps"
        },
        "temp_files": {
            "images": "./temp/images.json",
            "fonts": "./temp/fonts.json",
            "file_modification_time": "./temp/partials_modification_time.json"
        },
        "web_paths": {
            "css": "/library/css/",
            "fonts": "/library/fonts/",
            "img": "/library/img/",
            "js": "/library/js/"
        }
        
        
        
    }
}